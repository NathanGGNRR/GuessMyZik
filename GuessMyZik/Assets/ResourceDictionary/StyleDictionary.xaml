<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="MainColor">#016FB9</Color>
    <Color x:Key="MainDarkColor">#005B8E</Color>
    
    <Color x:Key="WriteColor">#F2F2F2</Color>
    <Color x:Key="WriteDarkColor">#FFC8C8C8</Color>
    <Color x:Key="TransparentColor">#00000000</Color>
    
    <Color x:Key="SecondaryColor">#AFA98D</Color>
    <Color x:Key="SecondaryDarkColor">#9B9680</Color>
    
    <Color x:Key="ThirdColor">#6D8EA0</Color>
    <Color x:Key="ThirdDarkColor">#557989</Color>
    <Color x:Key="ThirdLightColor">#FF99ACB6</Color>
    <Color x:Key="ThirdWhiteColor">#FFC3D5DE</Color>
    
    <Color x:Key="DarkColor">#182825</Color>
    <Color x:Key="LightBlue">#22AED1</Color>

    <Color x:Key="ExitColor">#FFAE0B0B</Color>
    <Color x:Key="ExitDarkColor">#6e0707</Color>

    <Color x:Key="LastColor">#22aed1</Color>
    <Color x:Key="LastDarkColor">#187c95</Color>

    <Color x:Key="TextBoxBorderBrush">#66000000</Color>

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Assets/ResourceDictionary/ButtonStyleDictionary.xaml" />
        <ResourceDictionary Source="/Assets/ResourceDictionary/TextBoxStyleDictionary.xaml" />
        <ResourceDictionary Source="/Assets/ResourceDictionary/ListViewStyleDictionary.xaml" />
    </ResourceDictionary.MergedDictionaries>
    
</ResourceDictionary>
